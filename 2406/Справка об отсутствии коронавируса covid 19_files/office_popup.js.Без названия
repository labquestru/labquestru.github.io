$(document).ready((function(){$(".city-search-input").on("keyup",(function(){let t=$(this).val();$(".city-search-list a").each((function(){-1===$(this).text().toLowerCase().search(t.toLowerCase())?($(this).hide(),$(this).addClass("hide")):($(this).show(),$(this).removeClass("hide"))})),$(".city-search-list .letter").each((function(){0==$(this).find("a:not(.hide)").length?$(this).hide():$(this).show()}))}))}));